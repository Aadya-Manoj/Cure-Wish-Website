<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>curelogin</servlet-name>
        <servlet-class>curelogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BloodOxygenLevelTestServlet</servlet-name>
        <servlet-class>BloodOxygenLevelTestServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BloodPressureCalculatorServlet</servlet-name>
        <servlet-class>BloodPressureCalculatorServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CholesterolTestCalculatorServlet</servlet-name>
        <servlet-class>CholesterolTestCalculatorServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FastBloodTestCalculatorServlet</servlet-name>
        <servlet-class>FastBloodTestCalculatorServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>LogoutServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>curelogin</servlet-name>
        <url-pattern>/curelogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BloodOxygenLevelTestServlet</servlet-name>
        <url-pattern>/BloodOxygenLevelTestServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BloodPressureCalculatorServlet</servlet-name>
        <url-pattern>/BloodPressureCalculatorServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CholesterolTestCalculatorServlet</servlet-name>
        <url-pattern>/CholesterolTestCalculatorServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FastBloodTestCalculatorServlet</servlet-name>
        <url-pattern>/FastBloodTestCalculatorServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
